{
  "name": "richmd-react",
  "version": "0.3.0",
  "description": "Use Richmd for React.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
