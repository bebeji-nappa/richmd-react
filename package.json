    "lint:eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --config .eslintrc.js",
    "lint:eslint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --config .eslintrc.js",
    "lint:prettier": "pnpm prettier --check './**/*.{js,jsx,ts,tsx,css}'",
    "lint:prettier:fix": "pnpm prettier --write './**/*.{js,jsx,ts,tsx,css}'",
    "test": "jest"
  },
  "keywords": [
    "React",
    "richmd",
    "markdown",
    "html",
    "parser",
    "math"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bebeji-nappa/richmd-react.git"
  },
  "author": "nappa <masa.yanagawa721@gmail.com> (https://twitter.com/nappa_bebeji)",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "richmd": "^2.2.16"
  },
  "volta": {
    "node": "20.9.0",
    "pnpm": "8.15.3"
